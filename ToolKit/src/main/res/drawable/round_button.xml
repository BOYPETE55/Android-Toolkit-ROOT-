<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
	<item android:state_pressed="true" >
		<shape android:shape="rectangle"  >
			<corners android:radius="25dip" />
			<stroke android:width="1dip" android:color="#DADADA" />
			<gradient android:angle="-90" android:startColor="#FFFFFF" android:endColor="#CACACA"  />            
		</shape>
	</item>
	<item android:state_focused="true">
		<shape android:shape="rectangle"  >
			<corners android:radius="25dip" />
			<stroke android:width="1dip" android:color="#FFFFFF" />
			<solid android:color="#F1F1F1"/>       
		</shape>
	</item>  
	<item >
		<shape android:shape="rectangle"  >
			<corners android:radius="25dip" />
			<stroke android:width="1dip" android:color="#CFCFCF" />
			<gradient android:angle="-90" android:startColor="#FFFFFF" android:endColor="#D2D8D8" />            
		</shape>
	</item>
</selector>
